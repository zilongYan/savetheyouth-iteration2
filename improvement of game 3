    <script type="text/javascript">
         var ReadyToPlay = document.getElementById('ReadyToPlay');
         var SelectChar = document.getElementById('SelectChar');
         var Loading = document.getElementById('Loading');
         var playGame = document.getElementById('play');
         var v1 = document.getElementById('1');
         var v2 = document.getElementById('2');
         var v3 = document.getElementById('3');
         var q1 = document.getElementById('q1');
         var q1button = document.getElementById('q1button');
         var q1abandon = document.getElementById('q1abandon');
         var q1respect = document.getElementById('q1respect');
         var q1a1 = document.getElementById('q1a1');
         var q1a2 = document.getElementById('q1a2');
         var q1res = document.getElementById('q1res');
         var q1reselectButton = document.getElementById('q1reselectButton');
         var q1select = document.getElementById('q1select');
         var choosetoy = document.getElementById('choosetoy');
         var bear = document.getElementById('bear');
         var dragon = document.getElementById('dragon');
         var choosetoyButton = document.getElementById('choosetoyButton');
         var selectbear = document.getElementById('selectbear');
         var aftershopping = document.getElementById('aftershopping');
         var backtohome = document.getElementById('backtohome');
         var toyselect = document.getElementById('toyselect');
         var rob = document.getElementById('rob');
         var exchange = document.getElementById('exchange');
         var robtoy = document.getElementById('robtoy');
         var toyreselect = document.getElementById('toyreselect');
         var toyreselection = document.getElementById('toyreselection');
         var exchangetoy = document.getElementById('exchangetoy');
         var losegame = document.getElementById('losegame');
         var throwdesk = document.getElementById('throwdesk');
         var losegamereselection = document.getElementById('losegamereselection');
         var reselectlosegame = document.getElementById('reselectlosegame');
         var chooseChar = document.getElementById('chooseChar');
         var selectelder = document.getElementById('selecteleder');
         var selectyounger = document.getElementById('selectyounger');
         var selectChar = document.getElementById('SelectChar');
        playGame.addEventListener("click", function(event) {
        ReadyToPlay.style.display = 'none' ;
        Loading.style.display = 'block' ;
            Loading.play();
            playGame.style.display= 'none'
}, false);
        
        Loading.onended = function(){
            Loading.style.display = 'none';
            selectChar.style.display = 'block';
            selectChar.play();
            chooseChar.style.display ='block';
//            v1.style.display = 'block';
//            v1.play();
            
        }
        selectelder.addEventListener('click',function(event){
            selectChar.style.display = 'none';
            chooseChar.style.display ='none';
            v1.style.display = 'block';
            v1.play(); 
        },false)
        
        
        selectyounger.addEventListener('click',function(event){
            selectChar.style.display = 'none';
            chooseChar.style.display ='none';
            document.getElementById('game3-younger-select1').style.display = 'block';
            document.getElementById('game3-younger-select1').play();
            
        },false)
         
        document.getElementById('game3-younger-select1').onended = function (){
           document.getElementById('younger-select1').style.display = 'block'
        }
        document.getElementById('stick').addEventListener('click',function(event){
             document.getElementById('game3-younger-reselect1').style.display ='none';
            document.getElementById('game3-younger-select1').style.display = 'none';
            document.getElementById('younger-select1').style.display = 'none';
            document.getElementById('game3-younger-goshopping').style.display = 'block';
            document.getElementById('game3-younger-goshopping').play();
        },false)
        
        document.getElementById('change').addEventListener('click',function(event){
            document.getElementById('game3-younger-reselect1').style.display ='none';
            document.getElementById('game3-younger-select1').style.display = 'none';
            document.getElementById('younger-select1').style.display = 'none';
            document.getElementById('game3-younger-select1-1').style.display = 'block';
            document.getElementById('game3-younger-select1-1').play();
        },false)
        
        document.getElementById('game3-younger-select1-1').onended = function (){
           document.getElementById('younger-reselect').style.display = 'block';
        }
        
        document.getElementById('younger-reselect-button').addEventListener('click',function(event){
            document.getElementById('game3-younger-select1-1').style.display = 'none';
            document.getElementById('younger-reselect').style.display = 'none';
            document.getElementById('younger-select1').style.display = 'block'
            document.getElementById('game3-younger-reselect1').style.display ='block';
            document.getElementById('game3-younger-reselect1').play();
        },false)
        
        document.getElementById('game3-younger-goshopping').onended = function(){
            document.getElementById('game3-younger-goshopping').style.display = 'none';
            document.getElementById('game3-younger-choose-toy').style.display = 'block';
            document.getElementById('game3-younger-choose-toy').play();
            document.getElementById('younger-choosetoyButton').style.display = 'block';
            
        }
        
        document.getElementById('younger-bear').addEventListener('click',function(event){
            document.getElementById('game3-younger-choose-toy').style.display = 'none';
            document.getElementById('younger-choosetoyButton').style.display = 'none';
            document.getElementById('game3-younger-choose-bear').style.display = 'block';
            document.getElementById('game3-younger-choose-bear').play();
        },false)
        document.getElementById('younger-dragon').addEventListener('click',function(event){
             document.getElementById('game3-younger-choose-toy').style.display = 'none';
            document.getElementById('younger-choosetoyButton').style.display = 'none';
            document.getElementById('game3-younger-choose-dragon').style.display = 'block';
            document.getElementById('game3-younger-choose-dragon').play();
        },false)
        
        document.getElementById('game3-younger-choose-bear').onended = function(){
             document.getElementById('game3-younger-choose-bear').style.display = 'none';
            document.getElementById('game3-younger-gohome').style.display = 'block';
            document.getElementById('game3-younger-gohome').play();
        }
        
         document.getElementById('game3-younger-choose-dragon').onended = function(){
             document.getElementById('game3-younger-choose-dragon').style.display = 'none';
            document.getElementById('game3-younger-gohome').style.display = 'block';
            document.getElementById('game3-younger-gohome').play();
        }
         
         document.getElementById('game3-younger-gohome').onended = function(){
             document.getElementById('game3-younger-gohome').style.display = 'none';
             document.getElementById('game3-younger-select2').style.display = 'block';
             document.getElementById('game3-younger-select2').play();
         }
         
         document.getElementById('game3-younger-select2').onended = function(){
            document.getElementById('younger-toyexchange').style.display = 'block';
         }
         document.getElementById('against').addEventListener('click',function(event){
              document.getElementById('game3-younger-reselect2').style.display = 'none'
             document.getElementById('younger-toyreselection').style.display = 'none';
             document.getElementById('younger-toyexchange').style.display = 'none';
              document.getElementById('game3-younger-select2').style.display = 'none';
             document.getElementById('game3-younger-select2-1').style.display = 'block';
              document.getElementById('game3-younger-select2-1').play();
         },false)
        
        
        document.getElementById('younger-exchange').addEventListener('click',function(event){
             document.getElementById('game3-younger-reselect2').style.display = 'none';
             document.getElementById('younger-toyexchange').style.display = 'none';
              document.getElementById('game3-younger-select2').style.display = 'none';
             document.getElementById('game3-younger-playing-chest').style.display = 'block';
              document.getElementById('game3-younger-playing-chest').play();
         },false)
        
         document.getElementById('game3-younger-select2-1').onended = function(){
             document.getElementById('younger-toyreselection').style.display = 'block';
         }
         
         document.getElementById('younger-reselecttoy').addEventListener('click',function(event){
